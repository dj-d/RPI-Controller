<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.PowerStripTwoFragment">

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/swipe_power_strip_two"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <Switch
                android:id="@+id/s2_switch_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/s2_switch_1"
                android:text="" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="10dp" />

            <Switch
                android:id="@+id/s2_switch_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/s2_switch_2"
                android:text="" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <Switch
                android:id="@+id/s2_switch_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/s2_switch_3"
                android:text="" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <Switch
                android:id="@+id/s2_switch_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/s2_switch_4"
                android:text="" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </LinearLayout>

    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

</FrameLayout>